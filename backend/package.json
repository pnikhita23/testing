{
  "name": "propio-backend",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "tsc --lib dom && serverless offline --noTimeout --noPrependStageInUrl --ignoreJWTSignature --host 0.0.0.0",
    "predeploy": "yarn compile",
    "deploy": "yarn compile && serverless deploy --stage dev",
    "coverage": "nyc --reporter lcov npm run test",
    "lint:verify": "yarn eslint src --ext .ts",
    "lint": "yarn eslint src/**/*.ts --fix --ext .ts",
    "format:verify": "yarn prettier --check **/*.{ts,md,json}",
    "format": "yarn prettier --write **/*.{ts,md,json}",
    "compile": "tsc --lib dom",
    "test": "tsc --lib dom && jest ",
    "test-ci": "tsc --lib dom && jest --ci --reporters=jest-junit --reporters=default"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.682.0",
    "@aws-sdk/client-sqs": "^3.670.0",
    "@langchain/openai": "^0.1.2",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@middy/core": "^2.0.0",
    "@middy/do-not-wait-for-empty-event-loop": "^2.0.0",
    "@middy/http-cors": "^2.0.0",
    "@middy/http-error-handler": "^2.0.0",
    "@middy/http-header-normalizer": "^2.0.0",
    "@middy/http-json-body-parser": "^2.0.0",
    "@sendgrid/helpers": "^8.0.0",
    "@sendgrid/mail": "^7.6.1",
    "@types/node": "^20.8.8",
    "@types/superagent": "^4.1.15",
    "applicationinsights": "^2.5.0",
    "dateformat": "4.6.3",
    "dotenv": "^16.0.0",
    "ioredis": "^5.4.1",
    "mailgen": "^2.0.20",
    "moment-timezone": "^0.5.38",
    "pg": "^8.13.0",
    "sqlite3": "^5.1.7",
    "superagent": "^7.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.10.0",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/aws-lambda": "^8.10.64",
    "@types/aws-sdk": "^2.7.0",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^26.0.24",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "babel-loader": "^8.2.2",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^7.23.0",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "husky": "^6.0.0",
    "jest": "29.6.2",
    "jest-junit": "^16.0.0",
    "lambda-tester": "^4.0.1",
    "prettier": "^2.2.1",
    "serverless": "3.40.0",
    "serverless-offline": "13.2.0",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-webpack": "^5.13.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^8.9.1",
    "typescript": "5.4.5",
    "webpack": "^5.30.0"
  }
